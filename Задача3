#include <iostream>


int sign(int x) {
    if (x > 0) return 1;
    else if (x == 0) return 0;
    else return -1;
}

int main() {
    int x, y;

    
    std::cout << "x: ";
    std::cin >> x;
    std::cout << "y: ";
    std::cin >> y;

    int z = sign(x) + sign(y);
    std::cout << "z = " << z << std::endl;

    return 0;
}
