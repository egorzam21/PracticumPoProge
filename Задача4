def is_in_arithmetic_progression(f, s, 

    if (p - f) % s == 0:
        return True
    else:
        return False

def main():
    f = int(input("Введите первый член арифметической прогрессии: "))
    s = int(input("Введите шаг арифметической прогрессии: "))
    p = int(input("Введите число p для проверки: "))

    if is_in_arithmetic_progression(f, s, p):
        print(f"{p} является членом арифметической прогрессии с первым членом {f} и шагом {s}.")
    else:
        print(f"{p} не является членом арифметической прогрессии с первым членом {f} и шагом {s}.")

if __name__ == "__main__":
    main()
