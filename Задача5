import random
 
def MIN(arr1, arr2):
    return [min(x, y) for x, y in zip(arr1, arr2)]
 
array1 = [random.randint(1, 100) for _ in range(5)]
array2 = [random.randint(1, 100) for _ in range(5)]

result = MIN(array1, array2)

print(f"Массив 1: {array1}")
print(f"Массив 2: {array2}")
print(f"Мининимальный массив: {result}")
